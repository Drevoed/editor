{
  "name": "@cardbox-editor/turborepo",
  "private": true,
  "devDependencies": {
    "turbo": "^1.0.2"
  },
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**"
        ]
      },
      "lint": {
        "outputs": []
      }
    }
  }
}